{
  "name": "@the-nerd-cave/paginate",
  "version": "1.1.0",
  "description": "Create a paginated array with a defined length of elements per page.",
  "keywords": [
    "pagination",
    "paginate",
    "array"
  ],
  "main": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "exports": {
    "import": "./esm/index.mjs",
    "require": "./dist/lib/index.js"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "lint": "eslint ./src ./test --ext .ts",
    "build": "rimraf dist && tsc",
    "test": "jest --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/example/"
    ],
    "collectCoverageFrom": [
      "src/*.{ts,js}"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acollierr17/paginate.git"
  },
  "author": "acollier17 <gamemaniac101@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/acollierr17/paginate/issues"
  },
  "homepage": "https://github.com/acollierr17/paginate#readme",
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.9.0",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.4.2",
    "jest-config": "^26.4.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  }
}
